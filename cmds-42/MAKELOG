================= make all for revnetgroup =================
rm -f Makedep
cc -M -DNeXT_MOD -D__STRICT_BSD__ revnetgroup.c getgroup.c table.c util.c   |  awk '{ if ($1 != prev) { if (rec != "") print rec;  rec = $0; prev = $1; }  else { if (length(rec $2) > 78) { print rec; rec = $0; }  else rec = rec " " $2 } }  END { print rec }' >Makedep
