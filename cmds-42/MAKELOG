================= make all for hostinfo =================
if [ -n "hostinfo" ]; then make hostinfo.all ; fi;
if [ `expr hostinfo : '.*\.a' + 1 ` -ne 1 ] ; then if [ -f hostinfo.NEW ] ; then ar ru hostinfo.NEW  hostinfo.o         ; else ar q hostinfo.NEW  hostinfo.o         ; fi elif [ -n "" ] ; then cp  hostinfo.NEW; elif [ -n "" ] ; then cp  hostinfo.NEW; else cc -O -MD -object -o hostinfo.NEW  hostinfo.o          -lsys_s; fi
for i in `echo `; do echo ======= make $i.all =======; make -f $i.mk PRODUCT=$i MAKEFILE=$i.mk $i.all; done;
CWD=`pwd`; cd ; DSTROOT=`pwd`; cd $CWD;  for i in `echo `;  do  echo ================= make all for $i =================;  (cd $i; make DSTROOT=$DSTROOT all);  done
