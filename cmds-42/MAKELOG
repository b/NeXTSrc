cc -M -DNeXT_MOD -D__STRICT_BSD__ spray.c   |  awk '{ if ($1 != prev) { if (rec != "") print rec;  rec = $0; prev = $1; }  else { if (length(rec $2) > 78) { print rec; rec = $0; }  else rec = rec " " $2 } }  END { print rec }' >Makedep
cc -O -g -fwritable-strings -DNeXT_MOD -D__STRICT_BSD__ -c spray.c
vers_string -c spray > vers.c
cc -O -g -fwritable-strings -DNeXT_MOD -D__STRICT_BSD__ -c vers.c
cc -O -g -fwritable-strings -DNeXT_MOD -D__STRICT_BSD__ -o spray.NEW spray.o   vers.o -lsys_s -lrpcsvc
================= make all for yp =================
################# make all for usr.etc/yp ###################
================= make all for makedbm =================
rm -f Makedep
cc -M -DNeXT_MOD -D__STRICT_BSD__ -I.. makedbm.c   |  awk '{ if ($1 != prev) { if (rec != "") print rec;  rec = $0; prev = $1; }  else { if (length(rec $2) > 78) { print rec; rec = $0; }  else rec = rec " " $2 } }  END { print rec }' >Makedep
cc -O -g -fwritable-strings -DNeXT_MOD -D__STRICT_BSD__ -I.. -c makedbm.c
vers_string -c makedbm > vers.c
cc -O -g -fwritable-strings -DNeXT_MOD -D__STRICT_BSD__ -I.. -c vers.c
cc -object -O -g -fwritable-strings -DNeXT_MOD -D__STRICT_BSD__ -I.. -o makedbm.NEW makedbm.o   vers.o -lsys_s -ldbm
