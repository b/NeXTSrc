	/bin/make DSTROOT=/release/NeXT_root LCLROOT=/os/osdev/NeXT_root DO\
	VERS=ALWAYS machine=next
rm -f Makedep
cc -DNeXT_MOD -DTELL -DVFORK -DCMUCS -D__STRICT_BSD__ -M dosetpath.c fold.c getbool.c printf.c prstab.c salloc.c  setpath.c sh.c sh.char.c sh.dir.c sh.dol.c sh.err.c sh.exec.c  sh.exp.c sh.file.c sh.func.c sh.glob.c sh.hist.c sh.init.c sh.lex.c  sh.misc.c sh.parse.c sh.print.c sh.proc.c sh.sem.c sh.set.c  sh.time.c skipto.c stablk.c stlmatch.c tenex.c   |  awk '{ if ($1 != prev) { if (rec != "") print rec;  rec = $0; prev = $1; }  else { if (length(rec $2) > 78) { print rec; rec = $0; }  else rec = rec " " $2 } }  END { print rec }' >Makedep
